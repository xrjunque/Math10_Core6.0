<Window x:Class="Output"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Math10_Core3._1"
        mc:Ignorable="d"
    Title="Output" Height="300" Width="300">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="0,0,0,0">
            <TextBox  Name="tbOutput" Margin="10,10,10,10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
        </ScrollViewer>
    </Grid>
</Window>
